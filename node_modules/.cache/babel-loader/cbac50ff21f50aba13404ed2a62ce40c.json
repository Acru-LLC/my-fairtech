g ? 'height:50px' : ''\"\r\n                            style=\"vertical-align: middle\"\r\n                          >\r\n                            <h5>\r\n                              {{ $t(\"messages.data_not_found\") }}\r\n                            </h5>\r\n                          </td>\r\n                        </tr>\r\n                        <tr\r\n                          :class=\"\r\n                            selectedTrItem.id == item.id\r\n                              ? 'bg-soft-primary text-primary'\r\n                              : item.status == 'CANCELED'\r\n                              ? 'bg-soft-danger'\r\n                              : ''\r\n                          \"\r\n                          class=\"p_cursor\"\r\n                          @click.prevent=\"clickTrItem(item)\"\r\n                          v-for=\"(item, index) in list\"\r\n                          :key=\"index + 'CREATE_DOC'\"\r\n                        >\r\n                          <td class=\"text-center\">\r\n                            <strong>\r\n                              {{ paginate(index, limit, page - 1) }}</strong\r\n                            >\r\n                          </td>\r\n                          <!-- <td class=\"text-center\">\r\n                            {{ item.regNumber }}\r\n                          </td> -->\r\n                          <td class=\"text-center\">\r\n                            {{ item.ownerLastName ? item.ownerLastName : '' }} {{ item.ownerFirstName ? item.ownerFirstName : '' }} {{ item.ownerParentName ? item.ownerParentName : '' }}\r\n                          </td>\r\n                          <td class=\"text-center\">\r\n                            {{ item.date }}\r\n                          </td>\r\n                          <td class=\"text-center\">\r\n                            <p class=\"pre m-0\">{{ item.fileName }}</p>\r\n                          </td>\r\n                          <!-- \r\n                          <td class=\"text-center\">\r\n                            {{\r\n                              getName({\r\n                                nameRu: item.docTypeNameRu,\r\n                                nameLt: item.docTypeNameLt,\r\n                              })\r\n                            }}\r\n                          </td> -->\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </b-overlay>\r\n                </div>\r\n                <div class=\"row\" v-if=\"total > 0\">\r\n                  <div class=\"col-lg-12\">\r\n                    <b-pagination\r\n                      size=\"sm\"\r\n                      class=\"m-0\"\r\n                      :total-rows=\"total\"\r\n                      :per-page=\"limit\"\r\n                      v-model=\"page\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"col-5\">\r\n              <div class=\"card card-body\">\r\n                <table class=\"table m-0\">\r\n                  <tbody>\r\n                    <tr v-if=\"selectedTrItem.id\">\r\n                      <td colspan=\"2\" class=\"text-muted text-center p-0\">\r\n                        <div class=\"p-4\">\r\n                          <b-button\r\n                            v-if=\"\r\n                              selectedTrItem.status == 'SIGNED' ||\r\n                              selectedTrItem.status == 'CANCELED'\r\n                            \"\r\n                            @click=\"viewIlova(selectedTrItem)\"\r\n                            variant=\"primary\"\r\n                          >\r\n                            <i class=\"fa fa-eye mr-1\"></i>\r\n                            {{ $t(\"actions.view\") }}\r\n                          </b-button>\r\n\r\n                          <div v-else>\r\n                            <b-button\r\n                              variant=\"primary\"\r\n                              class=\"ml-1\"\r\n                              @click=\"viewIlova(selectedTrItem)\"\r\n                            >\r\n                              <i class=\"fa fa-eye\"></i>\r\n                              {{ $t(\"actions.view\") }}\r\n                            </b-button>\r\n                            <!-- <b-button\r\n                              variant=\"light\"\r\n                              class=\"ml-2\"\r\n                              @click=\"editLetter(selectedTrItem)\"\r\n                            >\r\n                              <i class=\"fa fa-pencil-alt\"></i>\r\n                              {{ $t(\"actions.edit\") }}\r\n                            </b-button> -->\r\n                          </div>\r\n                        </div>\r\n                      </td>\r\n                    </tr>\r\n\r\n                    <!-- <tr>\r\n                      <td class=\"text-muted\">\r\n                        <b>\r\n                          {{ $t(\"docNumber\") }}\r\n                        </b>\r\n                      </td>\r\n                      <td>\r\n                        {{\r\n                          selectedTrItem.id && selectedTrItem.regNumber\r\n                            ? selectedTrItem.regNumber\r\n                            : \"\"\r\n                        }}\r\n                      </td>\r\n                    </tr> -->\r\n                    <tr>\r\n                      <td class=\"text-muted\">\r\n                        <b>\r\n                          {{ $t(\"docName\") }}\r\n                        </b>\r\n                      </td>\r\n                      <td>\r\n                        {{\r\n                          selectedTrItem.id && selectedTrItem.fileName\r\n                            ? selectedTrItem.fileName\r\n                            : \"\"\r\n                        }}\r\n                      </td>\r\n                    </tr>\r\n                    <tr>\r\n                      <td class=\"text-muted\">\r\n                        <b>\r\n                          {{ $t(\"docCreatedDate\") }}\r\n                        </b>\r\n                      </td>\r\n                      <td>\r\n                        {{\r\n                          selectedTrItem.id && selectedTrItem.created\r\n                            ? selectedTrItem.created\r\n                            : \"\"\r\n                        }}\r\n                      </td>\r\n                    </tr>\r\n\r\n                    <tr>\r\n                      <td class=\"text-muted\">\r\n                        <b>{{ $t(\"whoSent\") }}</b>\r\n                      </td>\r\n                      <td class=\"pl-0\">\r\n                        <ul\r\n                          v-if=\"selectedTrItem.id\"\r\n                          id=\"il\"\r\n                          class=\"list-unstyled chat-list\"\r\n                        >\r\n                          <li>\r\n                            <div\r\n                              class=\"media d-flex\"\r\n                              style=\"align-items: center\"\r\n                            >\r\n                              <div class=\"align-self-center mr-3\"></div>\r\n                              <div\r\n                                class=\"align-self-center mr-3\"\r\n                                v-if=\"selectedTrItem.ownerUploadPath\"\r\n                              >\r\n                                <img\r\n                                  height=\"auto\"\r\n                                  :src=\"`${hrUrl}/${selectedTrItem.ownerUploadPath}`\"\r\n                                  class=\"rounded-circle avatar-sm\"\r\n                                  alt\r\n                                />\r\n                              </div>\r\n                              <div\r\n                                class=\"avatar-sm align-self-center mr-3\"\r\n                                v-if=\"!selectedTrItem.ownerUploadPath\"\r\n                              >\r\n                                <span\r\n                                  class=\"\r\n                                    avatar-title\r\n                                    rounded-circle\r\n                                    bg-soft-primary\r\n                                    text-primary\r\n                                  \"\r\n                                  >{{\r\n                                    selectedTrItem.ownerFirstName.charAt(0)\r\n                                  }}</span\r\n                                >\r\n                              </div>\r\n\r\n                              <div class=\"media-body overflow-hidden\">\r\n                                <h5 class=\"font-size-14 mb-1\">\r\n                                  {{\r\n                                    `${selectedTrItem.ownerLastName} ${selectedTrItem.ownerFirstName} ${selectedTrItem.ownerParentName}`\r\n                                  }}\r\n                                </h5>\r\n                                <p class=\"m-0\">\r\n                                  {{\r\n                                    getName({\r\n                                      nameLt: selectedTrItem.depNameLt,\r\n                                      nameRu: selectedTrItem.depNameRu,\r\n                                      nameUz: selectedTrItem.depNameUz,\r\n                                    })\r\n                                  }}\r\n                                </p>\r\n                                <p class=\"m-0\">\r\n                                  {{\r\n                                    getName({\r\n                                      nameLt: selectedTrItem.positionNameLt,\r\n                                      nameRu: selectedTrItem.positionNameRu,\r\n                                      nameUz: selectedTrItem.positionNameUz,\r\n                                    })\r\n                                  }}\r\n                                </p>\r\n                              </div>\r\n                            </div>\r\n                          </li>\r\n                        </ul>\r\n                      </td>\r\n                    </tr>\r\n\r\n                    <tr v-if=\"filesList.length > 0\">\r\n                      <td class=\"text-muted\">\r\n                        <b>{{ $t(\"letter_ilovalar\") }}</b>\r\n                      </td>\r\n                      <td></td>\r\n                    </tr>\r\n\r\n                    <tr\r\n                      class=\"mt-3\"\r\n                      v-for=\"(item, index) in filesList\"\r\n                      :key=\"index + 'FILE'\"\r\n                    >\r\n                      <td class=\"p-2\" colspan=\"2\">\r\n                        <div\r\n                          class=\"\r\n                            d-flex\r\n                            align-items-center\r\n                            bg-light\r\n                            p-2\r\n                            justify-content-between\r\n                          \"\r\n                        >\r\n                          <div class=\"d-flex align-items-center\">\r\n                            <a\r\n                              @click=\"viewIlova(item)\"\r\n                              :download=\"\r\n                                getExt(item.uploadPath) == 'PDF' ||\r\n                                getExt(item.uploadPath) == 'pdf'\r\n                                  ? false\r\n                                  : item.uploadPath\r\n                              \"\r\n                              :href=\"\r\n                                getExt(item.uploadPath) == 'PDF' ||\r\n                                getExt(item.uploadPath) == 'pdf'\r\n                                  ? `#`\r\n                                  : `${baseUrl}/${item.uploadPath}`\r\n                              \"\r\n                            >\r\n                              <FileView\r\n                                class=\"my-card-hovered\"\r\n                                :uploadPath=\"item.fileName\"\r\n                              />\r\n                            </a>\r\n                            <span class=\"ml-2\">\r\n                              {{ item.fileName }}\r\n                            </span>\r\n                          </div>\r\n                          <a\r\n                            download\r\n                            :href=\"`${baseUrl}/${item.uploadPath}`\"\r\n                            class=\"mr-3 p-1 btn btn-sm btn-light\"\r\n                          >\r\n                            <i class=\"bx bx-download font-size-20\"></i>\r\n                          </a>\r\n                        </div>\r\n                      </td>\r\n                    </tr>\r\n\r\n                    <tr>\r\n                      <td class=\"text-muted\">\r\n                        <b>{{ $t(\"qabulQiluvchilar\") }}</b>\r\n                      </td>\r\n                      <td></td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n                <b-overlay :opacity=\"0.1\" :show=\"loaderReceiver\" rounded=\"sm\">\r\n                  <Receivers\r\n                    :selectedReview=\"infoData.review\"\r\n                    :selectedAgreement=\"infoData.agreement\"\r\n                    :selectedSignature=\"infoData.signature\"\r\n                  />\r\n                </b-overlay>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  <!-- </Layout> -->\r\n</template>\r\n\r\n<style >\r\n.bg-class {\r\n  background-color: #fbfbfb !important;\r\n}\r\n</style>"],"sourceRoot":"src/modules/letter/sent"}]}